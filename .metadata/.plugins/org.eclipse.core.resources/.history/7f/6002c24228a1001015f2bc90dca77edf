package com.jsp.jdbccrud;

import java.sql.Connection;
import java.sql.DriverManager;
//import java.sql.PreparedStatement;
import java.sql.SQLException;
import java.sql.Statement;
import java.util.Scanner;

public class personBatchExecution {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String url = "jdbc:postgresql://localhost:5432/personcrud";
		String user = "postgres";
		String password = "root";

		try (Connection con = DriverManager.getConnection(url, user, password); Scanner sc = new Scanner(System.in);) {

			Statement st = con.createStatement();

			String sql = "INSERT INTO person VALUES(10001,'Ganesh,33)";
			st.addBatch(sql);
			String sql1 = "INSERT INTO person VALUES(10002,'Dinesh,43)";
			st.addBatch(sql1);
			String sql2 = "INSERT INTO person VALUES(10003,'Yogesh,37)";
			st.addBatch(sql2);
			String sql3 = "INSERT INTO person VALUES(10004,'thagesh,23)";
			st.addBatch(sql3);

			st.executeBatch();

			System.out.println("Batch Execution Done...");

		} catch (SQLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}

}
